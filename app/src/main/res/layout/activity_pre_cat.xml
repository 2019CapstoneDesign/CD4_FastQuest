        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/pre_Food"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="320dp"
        android:background="@drawable/pre_food"
        android:checked="true"
        android:textOff="@null"
        android:textOn="@null"
        android:textSize="0sp"
        app:layout_constraintEnd_toStartOf="@+id/pre_Music"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toEndOf="@+id/pre_Meet"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/pre_Music"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="320dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/pre_music"
        android:checked="true"
        android:textOff="@null"
        android:textOn="@null"
        android:textSize="0sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/pre_Language"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="420dp"
        android:layout_marginEnd="270dp"
        android:background="@drawable/pre_language"
        android:checked="true"
        android:fillColor="#ffffff"
        android:textOff="@null"
        android:textOn="@null"
        android:textSize="0sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/pre_Camera"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="420dp"
        android:background="@drawable/pre_camera"
        android:checked="true"
        android:textOff="@null"
        android:textOn="@null"
        android:textSize="0sp"
        app:layout_constraintEnd_toStartOf="@+id/pre_Art"
        app:layout_constraintStart_toEndOf="@+id/pre_Language"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/pre_Art"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="420dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/pre_art"
        android:checked="true"
        android:textOff="@null"
        android:textOn="@null"
        android:textSize="0sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/pre_Game"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="520dp"
        android:background="@drawable/pre_game"
        android:checked="true"
        android:textOff="@null"
        android:textOn="@null"
        android:textSize="0sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/pre_travel_text"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_marginStart="40dp"
        android:text="@string/pre_travel_kor"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pre_Travel" />

    <TextView
        android:id="@+id/pre_book_text"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_marginStart="40dp"
        android:text="@string/pre_book_kor"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pre_Book" />

    <TextView
        android:id="@+id/pre_meet_text"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_marginStart="40dp"
        android:text="@string/pre_meet_kor"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pre_Meet" />

    <TextView
        android:id="@+id/pre_language_text"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_marginStart="40dp"
        android:text="@string/pre_language_kor"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pre_Language" />

    <TextView
        android:id="@+id/pre_sports_text"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:text="@string/pre_sports_kor"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/pre_movie_text"
        app:layout_constraintStart_toEndOf="@+id/pre_travel_text"
        app:layout_constraintTop_toBottomOf="@+id/pre_Sports" />

    <TextView
        android:id="@+id/pre_dance_text"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:text="@string/pre_dance_kor"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/pre_volnteer_text"
        app:layout_constraintStart_toEndOf="@+id/pre_book_text"
        app:layout_constraintTop_toBottomOf="@+id/pre_Dance" />

    <TextView
        android:id="@+id/pre_food_text"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:text="@string/pre_food_kor"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/pre_music_text"
        app:layout_constraintStart_toEndOf="@+id/pre_meet_text"
        app:layout_constraintTop_toBottomOf="@+id/pre_Food" />

    <TextView
        android:id="@+id/pre_camera_text"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:text="@string/pre_camera_kor"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/pre_art_text"
        app:layout_constraintStart_toEndOf="@+id/pre_language_text"
