<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#efe9cc"
    tools:context=".pre_cat">


    <TextView
        android:id="@+id/bar_text2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="122dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="122dp"
        android:text="@string/pre_cat_kor"
        android:textSize="25sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/pre_Travel"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="120dp"
        android:background="@drawable/pre_travel"
        android:checked="true"
        android:textOff="@null"
        android:textOn="@null"
        android:textSize="0sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/pre_Sports"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="120dp"
        android:background="@drawable/pre_sports"
        android:checked="true"
        android:textOff="@null"
        android:textOn="@null"
        android:textSize="0sp"
        app:layout_constraintEnd_toStartOf="@+id/pre_Movie"
        app:layout_constraintStart_toEndOf="@+id/pre_Travel"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/pre_Movie"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginTop="120dp"
        android:layout_marginEnd="40dp"
        android:background="@drawable/pre_movie"
        android:checked="true"
        android:textOff="@null"
        android:textOn="@null"
        android:textSize="0sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ToggleButton
        android:id="@+id/pre_Book"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="220dp"
        android:background="@drawable/pre_book"
        android:checked="true"
        android:textOff="@null"
        android:textOn="@null"
        android:textSize="0sp"
