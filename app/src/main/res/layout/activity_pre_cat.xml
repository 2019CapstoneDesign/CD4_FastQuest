    <TextView
        android:id="@+id/pre_game_text"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_marginStart="168dp"
        android:text="@string/pre_game_kor"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pre_Game" />

    <TextView
        android:id="@+id/pre_movie_text"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_marginEnd="30dp"
        android:text="@string/pre_movie_kor"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pre_Movie" />

    <TextView
        android:id="@+id/pre_volnteer_text"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_marginEnd="20dp"
        android:text="@string/pre_volunteer_kor"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pre_Volunteer" />

    <TextView
        android:id="@+id/pre_music_text"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_marginEnd="20dp"
        android:text="@string/pre_music_kor"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pre_Music" />

    <TextView
        android:id="@+id/pre_art_text"
        android:layout_width="90dp"
        android:layout_height="20dp"
        android:layout_marginEnd="20dp"
        android:text="@string/pre_art_kor"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pre_Art" />

    <Button
        android:id="@+id/saveButton"
        android:layout_width="55dp"
        android:layout_height="40dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="16dp"
        android:onClick="OnClickHandler"
        android:text="@string/save_kor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.882"
        app:layout_constraintStart_toEndOf="@+id/bar_text2"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/backButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:backgroundTint="#00333238"
        android:contentDescription="@string/back"
        android:scaleX="1"
        android:scaleY="2"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/ic_back" />

</androidx.constraintlayout.widget.ConstraintLayout>